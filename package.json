    "gatsby-source-filesystem": "^1.5.7",
    "gatsby-transformer-remark": "^1.7.19",
    "react": "^15.*",
    "react-dom": "^15.*",
    "react-bootstrap": "^0.31.5",
    "react-codemirror": "^1.0.0",
    "react-no-ssr": "^1.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "eslint": "^4.10.0",
